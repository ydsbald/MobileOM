{
  "expo": {
    "name": "MediTrack",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/icons.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-image-picker",
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.ydsbald.boltexponativewind",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e13c389d-6ea3-4505-9307-efcfbd8642ea"
      }
    },
    "owner": "avotriniainaadrien"
  }
}
